{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "camelcase": "off",
    "new-cap": "off",
    "max-len": ["error", { "code": 120 }]
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"]
}
